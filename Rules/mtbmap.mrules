			icon-image : Icons/mtbmap/first_aid.png
			icon-width : 17
			align-vertical : center
			min-zoom : 13
			painting-cycle : 810
		draw : icon
		define
			font-weight : normal
			font-size : 10
			font-stretch : 1
			text-offset-vertical : 60%
			min-zoom : 15
			text-align-vertical : near
			painting-cycle : 810
		draw : text	
	
		
	target: toilet
		define
			icon-image : Icons/mtbmap/toilet.png
			icon-width : 15:17
			align-vertical : center
			min-zoom : 15
		draw : icon

	target : power tower
		define
			min-zoom : 15
			shape : square
			shape-size : 15:4;16:6
			fill-color : #888
			line-style : none
		for : power=pole
			define
