import-script:..\Scripts\Maperipy\RoundElevation.py

features
	
	points, areas
		bikeshop : shop=bicycle OR service:bicycle:retail=yes
		bikepark : landuse=recreation_ground and sport=mtb
		cafe : amenity=cafe
		convenience : shop=convenience OR shop=supermarket
		food : amenity=restaurant OR amenity=fast_food
		lodging : tourism=guest_house OR tourism=hotel
		medical : (amenity=hospital OR amenity=clinic) AND NOT emergency=no
		worship church : amenity=place_of_worship AND religion=christian
		worship other : amenity=place_of_worship AND @isOneOf(religion, bahai, druze, budhist)
		worship mosque : amenity=place_of_worship AND religion=muslim
		worship synagogue : amenity=place_of_worship AND religion=jewish AND (tourism or historic)
		parking : amenity=parking
		archaeological : historic=archaeological_site
		ruins : historic=ruins
		attraction : tourism=attraction and not (amenity=place_of_worship OR amenity=restaurant)
		airport : aeroway=aerodrome
		gas : amenity=fuel
		camping : tourism=camp_site
	
		railway station : railway=station
		nature reserve : boundary=national_park OR boundary=protected_area OR leisure=nature_reserve
		
	//______________________________________________________________________________________________________________
			
	points
		place : place OR landuse=farmyard
		building point: building AND NOT building=entrance
		POI : @isOneOf(natural, peak, cave_entrance, tree) OR @isOneOf(historic, ruins, archaeological_site, memorial, monument) OR @isOneOf(man_made, tower, water_tower, communications_tower) OR tourism=viewpoint OR (tourism=attraction and not amenity=place_of_worship)
		power tower : @isOneOf(power, tower, pole, station, sub_station)
		picnic : leisure=picnic_table OR tourism=picnic_site
		barrier : barrier
		information : tourism=information
		turningcircle : highway=turning_circle
		toilet : amenity=toilets
		watersource : natural=spring OR man_made=water_well OR amenity=drinking_water OR man_made=cistern OR natural=waterhole OR waterway=waterfall
		
	//______________________________________________________________________________________________________________

	lines
		
		aerialway : aerialway AND NOT aerialway=station
		aeroway : aeroway
		railway : @isOneOf(railway,rail,light_rail,tram,subway,funicular)
		power line : @isOneOf(power, line, minor_line)
		stream : @isOneOf(waterway, stream, river, canal, drain, ditch, riverbank)
		coastline : natural=coastline
		cliff : natural=cliff OR landuse=quarry
		borders : boundary=administrative AND admin_level=2 AND NOT maritime=yes
		bridge : bridge AND NOT bridge=no
		wall : barrier=wall
		fence : way[barrier=fence] relation[route = fence AND @isMatch(name:en,"Israeli West Bank barrier")]
		cutline : man_made=cutline
		
		// contours
		contour major : contour[@isMulti(elevation, 100) AND NOT elevation=0 ]
		contour medium : contour[@isMulti(elevation, 20) AND NOT @isMulti(elevation, 100)]
		contour minor : contour[@isMulti(elevation, 10) AND NOT (@isMulti(elevation, 100) OR @isMulti(elevation, 20))]
		
		// roads
		road major motorway : @isOneOf(highway, motorway, motorway_link) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		road major trunk : @isOneOf(highway, trunk, trunk_link) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		road major primary : @isOneOf(highway, primary, primary_link) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		road major secondary : @isOneOf(highway, secondary, secondary_link) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		road street tertiary : @isOneOf(highway, tertiary, tertiary_link) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		road street residential: @isOneOf(highway, residential, living_street) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		road street unclassified: @isOneOf(highway, unclassified, service, road, minor) AND NOT @isMatch(is_in, "Jordan|Egypt|Syria|Lebanon")
		track : highway=track
		path : highway=path OR highway=cycleway OR highway=footway OR highway=bridleway
		//  background for cycle networks in MTB IL
		network cycle local : relation[network = lcn]
		network cycle regional : relation[network = rcn]
		network cycle national : relation[network = ncn]

		
//______________________________________________________________________________________________________________
			
	areas
		building : building
 		sports : leisure=stadium OR leisure=common OR leisure=track
		residential : landuse=residential
		area industrial : landuse=industrial OR @isOneOf(power,substation,sub_station,plant) OR (man_made=wastewater_plant) OR (power=station)
		area commercial : landuse=commercial OR landuse=retail
		area wood : landuse=forest OR natural=wood
		area glacier : natural=glacier
		area water : natural=water OR landuse=reservoir OR landuse=basin
		military : landuse=military OR military=danger_area
		construction : @isOneOf(landuse,construction,brownfield)
		archaeological: historic=archaeological_site
		area wetland : natural=wetland
		area farmland : landuse=farmland OR landuse=farm
		area grass : @isOneOf(landuse,grass,meadow,recreation_ground,cemetery,orchard,vineyard) OR @isOneOf(natural,grassland,heath,fell,tundra,scrub,moor) OR @isOneOf(leisure,golf_course,pitch,park,garden)
		area sand : natural=beach OR natural=sand
		breakwater : man_made=breakwater
		area island : @isOneOf(place,island,islet)
		pal A : relation[boundary=administrative AND name="Area A"]
		pal B : relation[boundary=administrative AND name="Area B"]
		quarry : landuse=quarry

//______________________________________________________________________________________________________________

properties
	curved : false
	font-family : Tahoma
	text-offset-vertical : -12%
	text-halo-color : #fff
	text-halo-width : 18%
	text-halo-opacity : 0.9
	text-line-spacing : 0.7
	//text : [[name:he]]
	map-background-color : #fff9ef
	map-background-opacity : 1
	map-sea-color : #B8DBF7
	line-end-cap : none
	line-start-cap : none
	line-opacity : 100%


	 
//______________________________________________________________________________________________________________
	
rules

// ___________________points________________________

	target : barrier
		define
			min-zoom : 15
			icon-width : 17
		for : barrier=gate
			for : access=no OR bicycle=no
				define
					icon-image : Icons/mtbmap/gate_closed.png
			else
				define
					icon-image : Icons/mtbmap/gate_open.png
		elsefor : barrier=kissing_gate OR barrier=stile
			define
				icon-image : Icons/mtbmap/gate_open.png
		elsefor : barrier=lift_gate
			define
				icon-image : Icons/mtbmap/gate_open.png
		elsefor : barrier=block OR barrier=debris
			define
				icon-image : none
		elsefor : barrier=cattle_grid
			define
				icon-image : Icons/mtbmap/cattle_grid.png
				icon-width: 22

		draw : icon
		//--
		

	target : place
		for : place=city
			define
				icon-image : Icons/mtbmap/dot.png
				icon-width : 7:5;8:8;9:9
				min-zoom : 7
				max-zoom : 9
			draw : icon
			define
				text-offset-vertical : 80%
				min-zoom : 8
				max-zoom : 14
				font-size : 8:12;10:16;14:20;15:22;16:24
				
		elsefor : place=town
			define
				min-zoom : 11
				font-size : 10:12;11:13;12:14;13:15;14:15;15:18;16:20
		elsefor : place=village
			define
				min-zoom : 12
				font-size : 13:10;16:20
		elsefor : place=hamlet
			define
				min-zoom : 14.5
				font-size : 14.5:10;16:16

		elsefor : (place=suburb OR place=quarter OR place=neighbourhood or place=borough)
			define
				font-size : 15:14;16:15
				min-zoom : 15
				text-color : white black 70%
		else
			stop
		define
			font-weight : bold
			font-family : tahoma
		// Hebrew preference for points & areas
		for : @isMatch(name, "[א-ת]")
			define
				text : name
		else
			define
				text : @any([[name:he]], [[name:en]], name)
		draw : text

	target : POI
		define
			icon-width : 17
			align-vertical : center

		for : natural=peak
			define
				min-zoom : 13
				icon-image : Icons/mtbmap/peak.png
		for : tourism=viewpoint
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/viewpoint.png
		for : tourism=attraction
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/star.png
		for : historic=ruins
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/ruins.png
		for : man_made=tower
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/tower.png
		for : (man_made=tower AND tower:type=communication)
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/antenna.png
		for : natural=cave_entrance
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/cave.png
				icon-width : 13
		for : man_made=water_tower	
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/water_tower.png
		for : natural=tree
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/tree.png
		for : historic=archaeological_site
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/jar.png
				icon-width : 13
		for : (historic=memorial OR historic=monument)
			define
				min-zoom : 14
				icon-image : Icons/mtbmap/monument.png
			for : (religion=muslim or religion=jewish)
				define
					icon-image : Icons/mtbmap/holy_place.png

		draw : icon
	
		define
			font-weight : normal
			font-size : 15:10;16:12
			font-stretch : 1
			text-offset-vertical : -10
			min-zoom : 15
			text-align-vertical : far
		for : @isMatch(name, "[א-ת]")
			define
				text : name
				text-direction : rtl
		elsefor : name:he
			define
				text : [[name:he]]
				text-direction : rtl
		elsefor : name:en
			define
				text : [[name:en]]
		else
			define
				text : name
			for : @isMatch(name, "[א-ת؀-ۿݐ-ݿ]")
				define
					text-direction : rtl
		for : NOT (natural=tree)
			draw : text


		define
			font-style : italic
			font-size : 15:10;16:12
			min-zoom : 15
			text-func : roundEle(e)
			text-align-vertical : near
			text-offset-vertical : 10
			text-align-horizontal : center
			text-line-spacing : 0.7
			text-color : black
			text-max-width : 15
			
		draw : text

	target: worship *
		if : * church
			define
				icon-image : Icons/mtbmap/church.png
		elseif : * mosque
			define
				icon-image : Icons/mtbmap/mosque.png
		elseif : * synagogue
			define
				icon-image : Icons/mtbmap/synagogue.png
		elseif : * other
			define
				icon-image : Icons/mtbmap/holy_place.png
		define
			icon-width : 13:17
		for : tourism=attraction
			define
				min-zoom : 13
		else
			define
				min-zoom : 15
		draw : icon
		
	target : parking
		define
			icon-image : Icons/mtbmap/parking.png
			icon-width : 15:12
			align-vertical : center
			min-zoom : 15
		for : access=private
			stop
		draw : icon
		
	target: cafe
		define
			icon-image : Icons/mtbmap/cup.png
			icon-width : 17
			align-vertical : center
			min-zoom : 14
		draw : icon
		define
			font-weight : normal
			text-color : black
			font-size : 14:9;15:10
			font-stretch : 1
			text-offset-vertical : 60%
			min-zoom : 15
			text-align-vertical : near
		for : @isMatch(name, "[א-ת]")
			define
				text : name
				text-direction : rtl
		elsefor : name:he
			define
				text : [[name:he]]
				text-direction : rtl
		elsefor : name:en
			define
				text : [[name:en]]
		else
			define
				text : name
			for : @isMatch(name, "[א-ת؀-ۿݐ-ݿ]")
				define
					text-direction : rtl
		draw : text
		

	target: food
		define
			icon-image : Icons/mtbmap/food.png
			icon-width : 17
			align-vertical : center
			min-zoom : 14
		draw : icon
		define
			font-weight : normal
			text-color : black
			font-size : 10
			font-stretch : 1
			text-offset-vertical : 60%
			min-zoom : 15
			text-align-vertical : near
		for : @isMatch(name, "[א-ת]")
			define
				text : name
				text-direction : rtl
		elsefor : name:he
			define
				text : [[name:he]]
				text-direction : rtl
		elsefor : name:en
			define
				text : [[name:en]]
		else
			define
				text : name
			for : @isMatch(name, "[א-ת؀-ۿݐ-ݿ]")
				define
					text-direction : rtl
		draw : text

	target: convenience
		define
			icon-image : Icons/mtbmap/shopping.png
			icon-width : 15:17
			align-vertical : center
			min-zoom : 15
		draw : icon


	target: lodging
		define
			icon-image : Icons/mtbmap/hotel.png
			icon-width : 17
			align-vertical : center
			min-zoom : 14
		draw : icon
		define
			font-weight : normal
			text-color : black
			font-size : 10
			font-stretch : 1
			text-offset-vertical : 60%
			min-zoom : 15
			text-align-vertical : near
		for : @isMatch(name, "[א-ת]")
			define
				text : name
				text-direction : rtl
		elsefor : name:he
			define
				text : [[name:he]]
				text-direction : rtl
		elsefor : name:en
			define
				text : [[name:en]]
		else
			define
				text : name
			for : @isMatch(name, "[א-ת؀-ۿݐ-ݿ]")
				define
					text-direction : rtl
		draw : text
		
	target: convenience
		define
			icon-image : Icons/mtbmap/shopping.png
			icon-width : 15:17
			align-vertical : center
			min-zoom : 15
		draw : icon


	target: bikeshop
		define
			icon-image : Icons/mtbmap/bike-red.png
			icon-width : 17
			align-vertical : center
			min-zoom : 13
			painting-cycle : 810
		draw : icon
		define
			font-weight : normal
			text-color : black
			font-size : 13:9;15:10
			font-stretch : 1
			text-offset-vertical : 60%
			min-zoom : 15
			text-align-vertical : near
			painting-cycle : 810
	
		for : @isMatch(name, "[א-ת]")
			define
				text : name
				text-direction : rtl
		elsefor : name:he
			define
				text : [[name:he]]
				text-direction : rtl
		elsefor : name:en
			define
				text : [[name:en]]
		else
			define
				text : name
			for : @isMatch(name, "[א-ת؀-ۿݐ-ݿ]")
				define
					text-direction : rtl
		draw : text
		
	target: bikepark
		define
			icon-image : Icons/mtbmap/MTBiker.png
			icon-width : 25
			align-vertical : center
			min-zoom : 8
			painting-cycle : 810
		draw : icon
		define
			font-weight : bold
